{
  "$schema": "./schemas/macOS-schema.json",
  "identifier": "main-capability",
  "description": "Capability for the main window",
  "windows": ["main", "import-account"],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "app:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "window:allow-set-title",
    "dialog:allow-open",
    "fs:allow-app-read"
  ]
}
